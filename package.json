{
  "name": "canchitas-backend",
  "version": "1.0.0",
  "description": "backend de reserva de canchitas",
  "main": "index.js",
  "repository": "https://github.com/ederivero/canchitas-webinar-backend.git",
  "author": "Eduardo Ramiro de Rivero Manrique <ederiveroman@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "dotenv -e .env -- ts-node-dev --respawn --transpile-only --exit-child --watch src src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "format": "prettier .",
    "format:write": "prettier . --write",
    "format:check": "format --check",
    "prisma:migrate:save": "prisma migrate dev --preview-feature --create-only",
    "prisma:migrate:run": "prisma migrate deploy --preview-feature",
    "prisma:migrate:run:test": "dotenv -e .env.test -- prisma migrate deploy --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:migrate:reset:test": "dotenv -e .env.test -- prisma migrate reset --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "prisma:format": "prisma format"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/swagger-ui-express": "^4.1.3",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "aws-sdk": "^2.1111.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv-cli": "^5.1.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.3.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
